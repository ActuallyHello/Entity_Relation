    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
      <h2>Таблица отчёта по студентам</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th>Операция</th>
              <th>Время</th>
              <th>Пользователь</th>
              <th>Зачетка</th>
              <th>ФИО</th>
              <th>Группа</th>
            </tr>
          </thead>
          <tbody>
            {{#each s}}
            <tr>
              <td>{{operation}}</td>
              <td>{{stamp}}</td>
              <td>{{userid}}</td>
              <td>{{id_s}}</td>
              <td>{{lname}} {{fname}} {{mname}}</td>
              <td>{{group_name}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
      <h2>Таблица отчёта по моделям</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th>Операция</th>
              <th>Время</th>
              <th>Пользователь</th>
              <th>id Модели</th>
              <th>Название модели</th>
              <th>Зачетка</th>
            </tr>
          </thead>
          <tbody>
            {{#each m}}
            <tr>
              <td>{{operation}}</td>
              <td>{{stamp}}</td>
              <td>{{userid}}</td>
              <td>{{id_m}}</td>
              <td>{{name_m}}</td>
              <td>{{id_s}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </main>
  </div>
</div>


