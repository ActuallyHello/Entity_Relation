    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
      <h2>Таблица модель-связь</h2>
      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th>Модель</th>
              <th>Связь</th>
              <th>Левая сущность</th>
              <th>Мощность</th>
              <th>Правая сущность</th>
            </tr>
          </thead>
          <tbody>
            {{#each vm}}
            <tr>
              <td>{{name_m}}</td>
              <td>{{name_be}}</td>
              <td>{{e_left}}</td>
              <td>{{power_be}}</td>
              <td>{{e_right}}</td>  
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </main>
  </div>
